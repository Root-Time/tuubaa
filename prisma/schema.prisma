generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Level {
  user  String @id @unique
  xp    Float
  lvl   Int
  guild String
}

model Rules {
  message String @id @unique
  channel String
  guild   String @unique
}
