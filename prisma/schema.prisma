generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Level {
  user  String @id @unique
  xp    Float
  lvl   Int
  guild String
}

model Rules {
  message String @id @unique
  channel String
  guild   String @unique
}

model RoleExplain {
  id    String @id @unique
  guild String @unique
}

model ticket {
  id     String  @id
  user   String
  claim  String?
  opened String
  closed String?
}

model YouTube {
  id String @id
  // posts String[] @unique
}
